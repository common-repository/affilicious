{
  "name": "affilicious/affilicious",
  "license": "GPL-2.0 or later",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "affiliate",
    "produkte",
    "product",
    "price comparison"
  ],
  "config": {
    "platform": {
      "php": "5.6"
    }
  },
  "autoload": {
    "psr-4": {
      "Affilicious\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/affilicious/carbon-fields"
    }
  ],
  "require": {
    "php": ">=5.6",
    "pimple/pimple": "^3.0",
    "htmlburger/carbon-fields": "1.4.3",
    "webmozart/assert": "^1.2"
  },
  "extra": {
    "installer-paths": {
      "./vendor/htmlburger/carbon-fields": [
        "htmlburger/carbon-fields"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
